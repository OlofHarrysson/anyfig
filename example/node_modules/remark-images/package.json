{
  "name": "remark-images",
  "version": "2.0.0",
  "description": "remark plugin to add an improved image syntax",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "mdx",
    "react",
    "jsx",
    "mdxast",
    "image"
  ],
  "homepage": "https://remark.js.org/",
  "repository": "remarkjs/remark-images",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "bugs": "https://github.com/remarkjs/remark-images/issues",
  "author": "John Otander <johnotander@gmail.com> (http://johnotander.com)",
  "contributors": [
    "John Otander <johnotander@gmail.com> (http://johnotander.com)",
    "Tim Neutkens <tim@zeit.co>",
    "Matija MarohniÄ‡ <matija.marohnic@gmail.com>",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Mathias Beugnon <mathias@beugnon.fr>"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "is-url": "^1.2.2",
    "unist-util-is": "^4.0.0",
    "unist-util-visit-parents": "^3.0.0"
  },
  "devDependencies": {
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark": "^11.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.0",
    "tape": "^4.0.0",
    "xo": "^0.28.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
